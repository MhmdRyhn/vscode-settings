{
    // START: Editor
    "editor.fontSize": 14,
    "editor.mouseWheelZoom": true,
    "editor.multiCursorModifier": "alt", // Other options: "ctrlCmd"
    "editor.rulers": [
        120
    ],
	"editor.quickSuggestions": {
    	"strings": true
	},
	// END: Editor

    // START: Files
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000, // "files.autoSave" must be "afterDelay"
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
	// END: Files

    // START: Git
    "git.branchProtection": [
        "master",
        "main",
        "stable"
    ],
    "git.terminalGitEditor": false,
	// END: Git

    // Estension.START: GoLang
    "go.lintOnSave": "file",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "[go]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
	// Estension.END: GoLang

	// Estension.START: Go Struct Tag Autocomplete & Generator
	"goStructTagAutogen.tagSuggestion": {
		"json": {
			"cases": ["snake"],
			"options": ["omitempty", "-"]
		},
		"mapstructure": {
			"cases": ["snake"],
			"options": ["omitempty"]
		},
		"gorm": {
			"cases": ["snake"]
		}
	},
	"goStructTagAutogen.generation": {
    	"template": "gorm:\"column:{{camel}}\""
	},
	"goStructTagAutogen.valueSuggestion": {
		"json": ["omitempty"]
	},
	// Estension.END: Go Struct Tag Autocomplete & Generator

    "openapi.tryit.insecureSslHostnames": [
        "localhost"
    ], // "window.autoDetectColorScheme" must be enabled

    // Estension.START: Todo Tree
    "todo-tree.regex.regex": "(//)\\s*($TAGS).*(\\n\\s*//\\s{1,}.*)*",
    "todo-tree.highlights.defaultHighlight": {
		"icon": "alert",
		"type": "text-and-comment",
		"foreground": "black",
		"background": "white",
		"opacity": 50,
		"iconColour": "blue",
		"gutterIcon": true,
	},
	"todo-tree.highlights.customHighlight": {
		"TODO": {
		    "icon": "check",
		    "foreground": "black",
		    "background": "white",
		    "iconColour": "white",
		},
		"NOTE": {
		    "icon": "note",
		    "foreground": "white",
		    "background": "gray",
		    "iconColour": "gray",
		},
		"COMMENT": {
		    "icon": "note",
		    "foreground": "white",
		    "background": "gray",
		    "iconColour": "gray",
		},
		"FIXME": {
		    "foreground": "black",
		    "background": "yellow",
		    "iconColour": "yellow",
		},
		"BUG": {
		    "foreground": "black",
		    "background": "red",
		    "iconColour": "red",
		},
		"DEPRECATED": {
		    "foreground": "black",
		    "background": "red",
		    "iconColour": "red",
		},
		"[ ]": {
		    "icon": "check",
		    "foreground": "black",
		    "background": "white",
		    "iconColour": "yellow",
		},
		"[x]": {
		    "icon": "check",
		    "foreground": "white",
		    "background": "green",
		    "iconColour": "green",
		}
	},
	"todo-tree.general.tags": [
		"BUG",
		"DEPRECATED",
		"HACK",
		"FIXME",
		"TODO",
		"NOTE",
		"COMMENT",
		"[ ]",
		"[x]"
	],
    // Estension.END: Todo Tree

    // Window Settings
    "window.autoDetectColorScheme": true,
    "window.zoomLevel": -1,
    // Workbench Settings
    "workbench.tree.indent": 25,
    "workbench.colorTheme": "Solarized Dark", // "window.autoDetectColorScheme" must be enabled
    "workbench.preferredDarkColorTheme": "Solarized Dark", // "window.autoDetectColorScheme" must be enabled

    "yaml.schemas": {
        "kubernetes": "*.yaml"
    }
}
